            "hash": "2c287b381c08080e5280a09c156fd597c20c198ad6927a7ac510d10a2f38f5d3",
            "extract_dir": "mGBA-build-2025-05-24-win64-8797-1d5bff49c9c581db2a3454eb03b3a963765cdee6"
        },
        "32bit": {
            "url": "https://s3.amazonaws.com/mgba/build/mGBA-build-2025-05-24-win32-8797-1d5bff49c9c581db2a3454eb03b3a963765cdee6.7z",
